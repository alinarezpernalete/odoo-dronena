<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="real_estate_property_tags_action" model="ir.actions.act_window">
        <field name="name">Property Tags</field>
        <field name="res_model">real_estate_property_tag</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="real_estate_property_tag_view_tree_form" model="ir.ui.view">
        <field name="name">Real Estate Property Tag Form</field>
        <field name="model">real_estate_property_tag</field>
        <field name="arch" type="xml">
            <form string="Formulario">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>